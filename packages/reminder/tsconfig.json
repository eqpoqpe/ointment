{
  "compilerOptions": {
    /* Projects */
    /* Language and Environment */
    "target": "ES2022",
    /* Modules */
    "module": "ESNext",
    "moduleResolution": "bundler",
    /* Interop Constraints */
    "forceConsistentCasingInFileNames": true,
    /* Type Checking */
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "dist"
  ]
}